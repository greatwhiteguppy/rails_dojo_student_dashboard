<div id="wrapper">
    <% if flash[:success] %>
        <div id="green">
            <p><%= flash[:success] %></p>
        </div>
    <% end %>
    <h1>Listing <%= @dojos.count %> Dojos</h1>
    <div id="dojo_table">
        <table>
            <thead>
                <th>Coding Dojo Branch</th>
                <th>Street</th>
                <th>City</th>
                <th>State</th>
                <th>Actions</th>
            </thead>
            <tbody>
                <tr>
                    <% @dojos.each do |dojo| %>
                    <td><%= dojo.branch %></td>
                    <td><%= dojo.street %></td>
                    <td><%= dojo.city %></td>
                    <td><%= dojo.state %></td>
                    <td><a href="/dojos/<%=dojo.id%>">Show</a> | <a href="/dojos/<%=dojo.id%>/edit">Edit</a> | <a href="/dojos/<%=dojo.id%>" data-method="delete">Destroy</a></td>
                </tr>
                <% end %>
            </tbody>
        </table>
    </div>
    <br> <a href="/dojos/new">New Dojo</a>
</div>
